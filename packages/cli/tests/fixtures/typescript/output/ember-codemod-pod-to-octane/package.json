{
  "name": "ember-codemod-pod-to-octane",
  "version": "0.0.0",
  "private": true,
  "description": "Small description for ember-codemod-pod-to-octane goes here",
  "keywords": [
    "codemod",
    "ember-codemod",
    "emberjs"
  ],
  "repository": "",
  "license": "MIT",
  "author": "",
  "type": "module",
  "main": "dist/src/index.js",
  "bin": "dist/bin/ember-codemod-pod-to-octane.js",
  "directories": {
    "test": "tests"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "sh build.sh --production",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" && pnpm format",
    "lint:format": "prettier . --cache --check",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "tsc --noEmit",
    "prepare": "pnpm build",
    "release:prepare": "changeset version",
    "release:publish": "pnpm build && changeset publish",
    "test": "sh build.sh --test && mt dist-for-testing --quiet"
  },
  "dependencies": {
    "@codemod-utils/files": "^3.2.3",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@codemod-utils/tests": "^2.2.2",
    "@ijlee2-frontend-configs/changesets": "^1.0.2",
    "@ijlee2-frontend-configs/eslint-config-node": "^2.3.3",
    "@ijlee2-frontend-configs/prettier": "^2.3.3",
    "@sondr3/minitest": "^0.1.2",
    "@tsconfig/node20": "^20.1.8",
    "@tsconfig/strictest": "^2.0.8",
    "@types/node": "^20.19.27",
    "@types/yargs": "^17.0.35",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.26.2",
  "engines": {
    "node": "20.* || >= 22"
  }
}
